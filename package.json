		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^1.0.7",
		"concurrently": "^7.0.0",
		"vite": "^2.8.0"
	}
}
